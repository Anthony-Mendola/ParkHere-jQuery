<div class="container">
  <div class="col-md-8 well">
    <h3>Welcome, <%= @user.name %>!</h3>
    <h4>Email: <%= @user.email %></h4>
    <%= User.most_listings.name %> is a SuperHost with the most listings!
  </div>

  <div id="userListingsShowInfo" class=" well col-sm-10">
    <h4>Your ParkHere Listings:</h4>
    <% @user.listings.each do |listing| %>
      <p><%= link_to listing.title, listing_path(listing) %>
      <div id="content-<%= listing.id %>"><%= listing.content.truncate(200) %>
        <a href='#' data-id ="<%= listing.id %>" class='js-more'>More Info</a>
      </div><br>
    <% end %>
  </div>
</div>
